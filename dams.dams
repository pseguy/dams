	LD	A,(DE)
	INC	DE
	CP	48		; "0"
	JR	C,FTVLP6	; if not a number
	CP	58		; "9" + 1
	JR	C,FTDEC		; it's a decimal
FTVLP6	CP	"#"		; hexa prefix ?
	JR	Z,FTHEX
	CP	"%		; binary prefix
	JR	Z,FTBIN
	CP	34		; '"' ascii prefix
	JR	Z,FTASC
	CP	"$		; the 'current program counter' symbol
	JR	Z,FTPC
	CP	":"		; ???
	JP	Z,FTPEEK
	DEC	DE
	BIT	4,(IX+F1)
	JR	NZ,FTVLP9
	CALL	FTLAB		; search label
	LD	H,B
	LD	L,C
	RET	Z
	BIT	7,(IX+F1)
	JP	NZ,ERR5
	BIT	1,(IX+F3)	; assembly phase ?
	JP	Z,ERR11
	JP	LABABS
