	LD	HL,BRKCPY	; storage for the original user's code
	LD	DE,(BRKPT)	; DE = requested breakpoint address
	LD	BC,3
	PUSH	BC
	LDIR			; Restore the breakpoint location original code
	POP	BC
	POP	HL
