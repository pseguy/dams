TSFID
	LD	HL,(EDC)	; End of code to be disassembled
	LD	DE,(PC)
	SCF
	SBC	HL,DE
	CCF
	EX	DE,HL
	RET
TSLMT
	LD	DE,(PC)
	LD	L,(IY+0)
	LD	H,(IY+1)
	SCF
	SBC	HL,DE
	EX	DE,HL
	RET
